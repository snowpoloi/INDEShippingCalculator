@model INDEShipping.Models.TransportCompany

@{
    ViewData["Title"] = "Upload XML";
}

<h2>Upload XML</h2>

<form asp-action="UploadXml" enctype="multipart/form-data">
    <div class="form-group">
        <label for="xmlFile">Select XML file to upload:</label>
        <input type="file" name="xmlFile" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Upload" class="btn btn-primary" />
    </div>
</form>

<div>
    <a asp-action="Index">Back to List</a>
</div>
